@{
    ViewData["Title"] = "RealEstate SearchEngine";
}

@model List<string>

<div class="container">
    <div class="row">
        <form class="form-inline" asp-controller="Search" asp-action="GetData" method="get">
            <label for="priceMin" class="sm-2">Price min</label>
            <input type="number" class="form-control sm-2" id="priceMin" name="priceMin" value="400000" />
            <label for="priceMax" class="sm-2">Price max</label>
            <input type="number" class="form-control sm-2" id="priceMax" name="priceMax" value="950000" />
            <input type="submit" id="search" value="Search" class="btn btn-primary" />
        </form>
    </div>
   @Html.Partial("ResultList", @Model)
</div>